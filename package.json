{
  "name": "websdk",
  "version": "0.0.2",
  "description": "Web SDK is a set of conventions and code to provide modularity and lazy loading for angular, polymer, and others under a unified and simplified build approach through webpack",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node ./samples/src/web/build/index.js --od ./samples/public/artifacts --w",
